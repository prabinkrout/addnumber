pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'master', url: 'https://github.com/prabinkrout/addnumber.git'
            }
        }
 stage('makejar') {
            steps {
                sh '''
                    echo "File pushed"
                    ls -l
                    java add.java

                '''
                
            }
        }
    }
}
